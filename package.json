    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-shopify": "^35.1.0",
    "husky": "^7.0.4",
    "nodemon": "^2.0.15",
    "tsc-watch": "^4.6.0",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
    "express-validator": "^6.14.0",
    "jsonwebtoken": "^8.5.1"
  },
