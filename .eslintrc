{
    "extends": "airbnb-base",
    "globals": {
      "fetch": false
    },
    "rules": {
      "indent": [
        "error",
        2
      ],
      "no-var": 2,
      "quotes": ["error", "single"],
      "semi": 2,
      "no-irregular-whitespace": 2,
      "no-trailing-spaces": 2,
      "no-eq-null": 2,
      "prefer-const": 2,
      "no-console": 0
    },
    "env": {
      "jest": true
    }
  }